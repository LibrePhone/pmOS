package() {
    mkdir -p "$pkgdir/usr/$_target"
    cd "$pkgdir/usr/$_target"
    tar -xf $srcdir/musl-$pkgver-r$pkgrel-$_arch.apk
    rm .PKGINFO .SIGN.*
